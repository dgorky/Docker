docker ps = list running containers
docker ps -a = list running and stopped container

docker pull redis = pulling image
docker run redis:4.0 = pulls image and starts container right away
docker run redis = running image
docker run -d redis = run image in detached mode
docker stop <CONTAINER ID> = stops the container
docker start = starts stopped container
docker run -p6000:6379 = run container + bind port
docker run -p6000:6379 redis
docker run -p6001:6379 redis:4.0
